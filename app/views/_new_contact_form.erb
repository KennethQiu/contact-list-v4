<form class="col s12">
  <div class="row">
    <div class="input-field col s12">
      <input id="first_name" type="text" class="validate">
      <label for="first_name">First Name</label>
    </div>

    <div class="input-field col s12">
      <input Name" id="last_name" type="text" class="validate">
      <label for="last_name">Last Name</label>
    </div>

    <div class="input-field col s12">
      <input id="mobile" type="text" class="validate">
      <label for="mobile">Mobile</label>
    </div>

    <div class="input-field col s12">
      <input id="email" type="email" class="validate">
      <label for="email">Email</label>
    </div>

    <div class="input-field col s12">
      <input URL" id="image_url" type="text" class="validate">
      <label for="image_url">Image URL</label>
    </div>
    <button id = 'new_contact_submit_button'class="btn waves-effect waves-light" type="submit" name="action">Submit
      <i class="material-icons right">send</i>
    </button>
  </div>
</form>